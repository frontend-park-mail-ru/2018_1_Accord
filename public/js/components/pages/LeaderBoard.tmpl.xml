<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <table>
        <tbody>
            <fest:for iterate="data.scoreBoard" index="i" value="v">
                <tr>
                    <td>
                        <fest:value>i + 1</fest:value>
                    </td>
                    <td>
                        <fest:value>v.nickname</fest:value>
                    </td>
                    <td>
                        <fest:value>v.rating</fest:value>
                    </td>
                </tr>
            </fest:for>
        </tbody>
    </table>
</fest:template>