<fest:template xmlns:fest="http://fest.mail.ru" context_name="navItems">
    <nav class="nav">
        <fest:for iterate="navItems" index="i" value="v">
            <a href="{v.href}" class="{v.class}">
                <fest:value>v.title</fest:value>
                <fest:space> </fest:space>
            </a>
        </fest:for>
        <input type="checkbox" class="nav__items-mute">Mute</input>
    </nav>
</fest:template>